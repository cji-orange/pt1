{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/js/config\\.js",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/js/config.js"
    },
    {
      "src": "/js/auth\\.js",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/js/auth.js"
    },
    {
      "src": "/js/app\\.js",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/js/app.js"
    },
    {
      "src": "/js/timer\\.js",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/js/timer.js"
    },
    {
      "src": "/js/history\\.js",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/js/history.js"
    },
    {
      "src": "/js/reports\\.js",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/js/reports.js"
    },
    {
      "src": "/js/settings\\.js",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/js/settings.js"
    },
    {
      "src": "/css/(.*)",
      "headers": { "Content-Type": "text/css" },
      "dest": "/css/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 